{% doc %}
  @prompt
    Create a modern footer section with multiple columns for links, contact information, and social media icons. Include sections for quick links, customer service links, company information, and a newsletter signup. Style it with a clean, organized layout that matches modern e-commerce design standards with proper spacing and typography., make 1 parent div and 5 child div which are flex and their name are mare jouel swimwear quick links our collection privacy & security and newsletter which make capitalize and at newsletter mail and subscribe button and in marejouel swimwear div down make another div which name is stay connected with the fb insta tiktok and twitter svg tag, make news letter also flex, newsletter is flex with top div like quick links our collection and other

{% enddoc %}
{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-footer-{{ ai_gen_id }} {
    background-color: {{ block.settings.background_color }};
    color: {{ block.settings.text_color }};
    padding: {{ block.settings.padding_top }}px 0 {{ block.settings.padding_bottom }}px;
  }

  .ai-footer-container-{{ ai_gen_id }} {
    max-width: 1200px;
    margin: 0 auto;
    padding: 0 20px;
    display: flex;
    gap: 40px;
    flex-wrap: wrap;
  }

  .ai-footer-column-{{ ai_gen_id }} {
    flex: 1;
    min-width: 200px;
    display: flex;
    flex-direction: column;
  }

  .ai-footer-heading-{{ ai_gen_id }} {
    font-size: {{ block.settings.heading_size }}px;
    font-weight: 600;
    margin: 0 0 20px 0;
    color: {{ block.settings.heading_color }};
    text-transform: capitalize;
    letter-spacing: 0.5px;
  }

  .ai-footer-link-list-{{ ai_gen_id }} {
    list-style: none;
    padding: 0;
    margin: 0;
    display: flex;
    flex-direction: column;}

  .ai-footer-link-item-{{ ai_gen_id }} {
    margin-bottom: 12px;
  }

  .ai-footer-link-{{ ai_gen_id }} {
    color: {{ block.settings.link_color }};
    text-decoration: none;
    font-size: {{ block.settings.text_size }}px;
    line-height: 1.5;
    transition: color 0.3s ease;
  }

  .ai-footer-link-{{ ai_gen_id }}:hover {
    color: {{ block.settings.link_hover_color }};
  }

  .ai-footer-stay-connected-{{ ai_gen_id }} {
    margin-top: 30px;
  }

  .ai-footer-stay-connected-title-{{ ai_gen_id }} {
    font-size: {{ block.settings.text_size }}px;
    font-weight: 600;
    margin: 0 0 15px 0;
    color: {{ block.settings.heading_color }};
    text-transform: capitalize;
  }

  .ai-footer-social-{{ ai_gen_id }} {
    display: flex;
    gap: 16px;
  }

  .ai-footer-social-link-{{ ai_gen_id }} {
    display: flex;
    align-items: center;
    justify-content: center;
    width: {{ block.settings.social_icon_size }}px;
    height: {{ block.settings.social_icon_size }}px;
    background-color: {{ block.settings.social_background_color }};
    color: {{ block.settings.social_icon_color }};
    border-radius: 50%;
    text-decoration: none;
    transition: all 0.3s ease;
  }

  .ai-footer-social-link-{{ ai_gen_id }}:hover {
    background-color: {{ block.settings.social_hover_background }};
    color: {{ block.settings.social_hover_color }};
    transform: translateY(-2px);
  }

  .ai-footer-social-link-{{ ai_gen_id }} svg {
    width: calc({{ block.settings.social_icon_size }}px * 0.5);
    height: calc({{ block.settings.social_icon_size }}px * 0.5);
  }

  .ai-footer-newsletter-{{ ai_gen_id }} {
    display: flex;
    flex-direction: column;
  }

  .ai-footer-newsletter-form-{{ ai_gen_id }} {
    display: flex;
    gap: 8px;
    margin-bottom: 16px;
  }

  .ai-footer-newsletter-input-{{ ai_gen_id }} {
    flex: 1;
    padding: 12px 16px;
    border: 1px solid {{ block.settings.input_border_color }};
    border-radius: {{ block.settings.input_border_radius }}px;
    background-color: {{ block.settings.input_background_color }};
    color: {{ block.settings.text_color }};
    font-size: {{ block.settings.text_size }}px;
  }

  .ai-footer-newsletter-input-{{ ai_gen_id }}::placeholder {
    color: {{ block.settings.placeholder_color }};
  }

  .ai-footer-newsletter-button-{{ ai_gen_id }} {
    padding: 12px 24px;
    background-color: {{ block.settings.button_color }};
    color: {{ block.settings.button_text_color }};
    border: none;
    border-radius: {{ block.settings.button_border_radius }}px;
    font-size: {{ block.settings.text_size }}px;
    font-weight: 600;
    cursor: pointer;
    transition: background-color 0.3s ease;
    white-space: nowrap;
  }

  .ai-footer-newsletter-button-{{ ai_gen_id }}:hover {
    background-color: {{ block.settings.button_hover_color }};
  }

  .ai-footer-newsletter-success-{{ ai_gen_id }} {
    color: {{ block.settings.success_color }};
    font-size: {{ block.settings.text_size | minus: 2 }}px;
    margin-top: 8px;
  }

  .ai-footer-newsletter-error-{{ ai_gen_id }} {
    color: {{ block.settings.error_color }};
    font-size: {{ block.settings.text_size | minus: 2 }}px;
    margin-top: 8px;
  }

  @media screen and (max-width: 768px) {
    .ai-footer-container-{{ ai_gen_id }} {
      flex-direction: column;
      gap: 30px;
    }

    .ai-footer-newsletter-form-{{ ai_gen_id }} {
      flex-direction: column;
    }
  }

  @media screen and (max-width: 480px) {
    .ai-footer-container-{{ ai_gen_id }} {
      padding: 0 16px;
    }
  }
{% endstyle %}

